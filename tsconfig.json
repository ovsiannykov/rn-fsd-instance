{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"@app/*": ["src/app/*"],
			"@shared/*": ["src/shared/*"],
			"@screens/*": ["src/screens/*"],
			"@widgets/*": ["src/widgets/*"],
			"@features/*": ["src/features/*"],
			"@entities/*": ["src/entities/*"],
			"@navigation/*": ["src/navigation/*"],
			"@core/*": ["src/core/*"]
		}
	}
}
