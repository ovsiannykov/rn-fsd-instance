{
	"name": "fsdMobileInstance",
	"version": "0.0.1",
	"private": true,
	"author": "Arbipay",
	"scripts": {
		"packager": "react-native start --reset-cache",
		"pods": "cd ios && pod install",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"android-release": "react-native run-android --variant=release",
		"ios-release": "npx react-native run-ios --configuration Release",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
		"lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"start": "react-native start",
		"test": "jest",
		"prepare": "husky install",
		"format": "prettier ./src --write"
	},
	"dependencies": {
		"@feature-sliced/eslint-config": "^0.1.0-beta.6",
		"babel-plugin-module-resolver": "^5.0.0",
		"husky": "^8.0.3",
		"react": "18.2.0",
		"react-native": "0.72.5"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/eslint-config": "^0.72.2",
		"@react-native/metro-config": "^0.72.11",
		"@tsconfig/react-native": "^3.0.0",
		"@types/react": "^18.0.24",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.7.0",
		"eslint": "^8.50.0",
		"jest": "^29.7.0",
		"metro-react-native-babel-preset": "^0.76.8",
		"prettier": "^2.8.8",
		"react-test-renderer": "^18.2.0",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">=16"
	},
	"lint-staged": {
		"*.{ts,json,tsx}": "npm run format",
		"*.{ts,tsx}": "npm run lint:fix"
	}
}
