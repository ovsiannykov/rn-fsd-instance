{
	"name": "fsd-mobile-instance",
	"version": "0.0.1",
	"private": true,
	"author": "Arbipay",
	"scripts": {
		"packager": "react-native start --reset-cache",
		"pods": "cd ios && pod install",
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"android-release": "react-native run-android --variant=release",
		"ios-release": "npx react-native run-ios --configuration Release",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
		"lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"start": "react-native start",
		"test": "jest --passWithNoTests",
		"prepare": "husky install",
		"format": "prettier ./src --write"
	},
	"dependencies": {
		"@feature-sliced/eslint-config": "^0.1.0-beta.6",
		"@os-team/i18next-react-native-language-detector": "^1.0.28",
		"@react-native-masked-view/masked-view": "^0.2.9",
		"@react-navigation/native": "^6.1.8",
		"@react-navigation/native-stack": "^6.9.14",
		"@react-navigation/stack": "^6.3.18",
		"axios": "^1.5.1",
		"babel-plugin-module-resolver": "^5.0.0",
		"date-fns": "^2.30.0",
		"i": "^0.3.7",
		"i18next": "^23.5.1",
		"lint-staged": "^14.0.1",
		"npm": "^10.1.0",
		"react": "18.2.0",
		"react-i18next": "^13.2.2",
		"react-native": "0.72.5",
		"react-native-device-info": "^10.11.0",
		"react-native-encrypted-storage": "^4.0.3",
		"react-native-gesture-handler": "^2.13.1",
		"react-native-safe-area-context": "^4.7.2",
		"react-native-screens": "^3.25.0",
		"react-native-svg": "^13.14.0",
		"zustand": "^4.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.20.0",
		"@babel/preset-env": "^7.20.0",
		"@babel/runtime": "^7.20.0",
		"@react-native/eslint-config": "^0.72.2",
		"@react-native/metro-config": "^0.72.11",
		"@tsconfig/react-native": "^3.0.0",
		"@types/react": "^18.0.24",
		"@types/react-test-renderer": "^18.0.0",
		"babel-jest": "^29.7.0",
		"eslint": "^8.50.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"jira-prepare-commit-msg": "^1.7.2",
		"metro-react-native-babel-preset": "^0.76.8",
		"prettier": "^2.8.8",
		"react-native-svg-transformer": "^1.1.0",
		"react-test-renderer": "^18.2.0",
		"typescript": "4.8.4"
	},
	"engines": {
		"node": ">=16"
	},
	"lint-staged": {
		"*.{ts,json,tsx}": "npm run format",
		"*.{ts,tsx}": "npm run lint:fix"
	}
}
